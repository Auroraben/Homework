#include <iostream> 
using namespace std; 

int main() { 
    const int row = 3;
    const int column = 3;
    int iarr[row][column]; 

    for(int i = 0; i < row; i++) 
        for(int j = 0; j < column; j++) 
            iarr[i][j] = (i+1) * (j+1); 

    for(int i = 0; i < row; i++) { 
        for(int j = 0; j < column; j++) 
            cout << iarr[i][j] << "\t"; 
        cout << endl; 
    } 

    return 0; 
}
